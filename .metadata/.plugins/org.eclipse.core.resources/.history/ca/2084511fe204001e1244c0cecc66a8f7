package com.mysite.cart_item;

import java.util.List;

import org.springframework.web.bind.annotation.GetMapping;

import ch.qos.logback.core.model.Model;


@con
public class Cart_item_Controller {

	private final Cart_item_Service cart_item_Service;

	
	
	@GetMapping("/detail/{product_id}/order")
	public String list(Model model) {
		List<Cart_item> cart_item = this.cart_item_Service.getList();
		 model.addAttribute("cart_item_Service", cart_item_Service);
		return "cart_item";
		
		
	}

}
